{
  "name": "rush-mls-api",
  "version": "1.0.0",
  "description": "mls service with bright source",
  "main": "index.ts",
  "scripts": {
    "test": "jest",
    "lint": "eslint src/ --ext .js,.ts",
    "format": "eslint src/ --ext .js,.ts --fix",
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist build",
    "watch": "tsc-watch --onSuccess 'node --inspect=0.0.0.0 --require dotenv/config dist/index.js'",
    "start": "node --optimize-for-size --max-old-space-size=460 --require dotenv/config dist/index.js",
    "seed": "node --require dotenv/config  ./node_modules/knex --knexfile ./knexfile.ts seed:run",
    "migrate": "node --require dotenv/config ./node_modules/knex --knexfile ./knexfile.ts migrate:latest",
    "migrate:rollback": "node --require dotenv/config ./node_modules/knex --knexfile ./knexfile.ts migrate:rollback",
    "postinstall": "npm run lint && tsc",
    "tsoa:gen": "tsoa swagger -c ./tsoa/tsoa.json && tsoa routes -c ./tsoa/tsoa.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/leobit/rush-realty/rush-mls-api.git"
  },
  "keywords": [
    "mls",
    "rets",
    "rushrealty",
    "rushhome"
  ],
  "author": "Mykola Panchenko",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/leobit/rush-realty/rush-mls-api/issues"
  },
  "homepage": "https://gitlab.com/leobit/rush-realty/rush-mls-api#readme",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@hapi/joi-date": "^2.0.1",
    "@types/jest": "^26.0.24",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "db-errors": "^0.2.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "http": "0.0.1-security",
    "http-status-codes": "^2.1.4",
    "jest": "^27.0.6",
    "jest-mock": "^27.0.6",
    "knex": "^0.95.6",
    "lodash": "^4.17.21",
    "pg": "^8.6.0",
    "redis": "^3.1.2",
    "swagger-ui-express": "^4.1.6",
    "typescript-ioc": "^3.2.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/hapi__joi": "^17.1.7",
    "@types/lodash": "^4.14.171",
    "@types/node": "^16.3.0",
    "@types/redis": "^2.8.31",
    "@types/request": "^2.48.6",
    "@types/request-promise": "^4.1.48",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/eslint-plugin-tslint": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "factory": "^1.0.4",
    "husky": "^7.0.1",
    "prettier": "^2.3.2",
    "prettier-eslint": "^12.0.0",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "tsc-watch": "^4.4.0",
    "tslint": "^6.1.3",
    "tsoa": "^2.4.3",
    "typescript": "^4.3.5",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": "^14.17.0",
    "npm": "^6.14.13"
  }
}
